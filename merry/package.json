{
  "name": "merry",
  "version": "1.0.0",
  "description": "Web app online at https://uwreact.ca",
  "author": "UW REACT <director@uwreact.ca>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development webpack-dashboard -- webpack-dev-server --hot --open --mode development",
    "test": "jest",
    "build": "yarn build:development",
    "build:development": "NODE_ENV=development webpack --mode production",
    "build:staging": "NODE_ENV=staging webpack --mode production",
    "build:production": "NODE_ENV=production webpack --mode production",
    "analyze": "yarn build --analyze",
    "deploy": "yarn deploy:development",
    "deploy:development": "yarn build:development && firebase use shire-development && firebase deploy --only hosting",
    "deploy:staging": "yarn build:staging && firebase use shire-staging && firebase deploy --only hosting",
    "deploy:production": "yarn build:production && firebase use shire-production && firebase deploy --only hosting"
  },
